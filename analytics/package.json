{
  "name": "ridges-analytics",
  "version": "1.0.0",
  "description": "Analyse data generate from the ridges website",
  "main": "app.js",
  "scripts": {
    "client-install": "yarn add --prefix client",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "cd client && yarn start --prefix client",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "author": "Precious Solomon Kayili",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.12"
  }
}
